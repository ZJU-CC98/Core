<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<Description>This package is used to retrieve CC98 identity information generated by CC98 exteral authentication service.</Description>
		<Copyright>Copyright (c) CC98 2016. All rights are reserved.</Copyright>
		<AssemblyTitle>CC98 External Identity Package</AssemblyTitle>
		<NeutralLanguage>zh-CN</NeutralLanguage>
		<Authors>CC98</Authors>
		<TargetFrameworks>netstandard1.3;net451</TargetFrameworks>
		<AssemblyName>CC98.Identity.External</AssemblyName>
		<PackageId>CC98.Identity.External</PackageId>
		<PackageTags>CC98</PackageTags>
		<PackageReleaseNotes>Update to ASP.NET Core RTM</PackageReleaseNotes>
		<PackageProjectUrl>https://github.com/sgjsakura/CC98</PackageProjectUrl>
		<PackageLicenseUrl>http://www.apache.org/licenses/LICENSE-2.0</PackageLicenseUrl>
		<NetStandardImplicitPackageVersion>1.6.0</NetStandardImplicitPackageVersion>
		<PackageTargetFallback Condition=" '$(TargetFramework)' == 'netstandard1.3' ">$(PackageTargetFallback);dnxcore50</PackageTargetFallback>
		<GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
		<GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
		<GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\CC98.Identity.Core\CC98.Identity.Core.csproj" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.Identity" Version="1.0.2" />
		<PackageReference Include="Microsoft.Extensions.Options" Version="1.0.2" />
		<PackageReference Include="JetBrains.Annotations" Version="10.1.4" />
	</ItemGroup>

	<PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
	</PropertyGroup>

	<ItemGroup Condition=" '$(TargetFramework)' == 'net451' ">
		<Reference Include="System" />
		<Reference Include="Microsoft.CSharp" />
	</ItemGroup>

</Project>